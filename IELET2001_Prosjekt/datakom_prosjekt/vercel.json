{
  "version": 2,
  "builds": [
    {
      "src": "datakom_prosjekt/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.11.6", "maxDuration": 30 }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "static"
      }
    }
  ],
  "routes": [
    {
      "src": "/IELET2001_Prosjekt/datakom_prosjekt/static/(.*)",
      "dest": "/IELET2001_Prosjekt/datakom_prosjekt/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "datakom_prosjekt/wsgi.py"
    }
  ]
}